# file: examples/list.road

// Showcasing list literals in Roadman.

say("--- List of words ---");
gimme my_mandem = ["Skepta", "JME", "Giggs", "Stormzy"];
say("My mandem includes:");
say(my_mandem);

say(""); // For a newline

say("--- List of digits ---");
gimme lotto_picks = [7, 12, 19, 25, 33, 49];
say("This week's lotto picks could be:");
say(lotto_picks);
